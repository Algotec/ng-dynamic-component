{
  "name": "ng-dynamic-component",
  "version": "2.3.0-algotec.3",
  "description": "Dynamic components with full life-cycle support for inputs and outputs for Angular",
  "main": "bundles/ng-dynamic-component.umd.js",
  "module": "bundles/ng-dynamic-component.es5.js",
  "es2015": "bundles/ng-dynamic-component.es2015.js",
  "typings": "ng-dynamic-component.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "ng",
    "dynamic",
    "component",
    "input",
    "output",
    "life-cycle"
  ],
  "author": "Alex Malkevich <malkevich.alex@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/core": "^5.0.0",
    "rxjs": "^5.0.0",
    "tslib": "^1.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "http://artifactory.algotec.co.il:8080/artifactory/api/npm/npm-virtual"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "collectCoverageFrom": [
      "src/dynamic/**/*.ts",
      "!src/dynamic/index.ts",
      "!src/dynamic/dynamic.module.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
